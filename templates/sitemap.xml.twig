<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    <url>
        <loc>https://www.askaway.fr/</loc>
        <changefreq>weekly</changefreq>
        <priority>1.0</priority>
    </url>

    {% for url in urls %}
        <url>
            <loc>{{ url }}</loc>
            <changefreq>weekly</changefreq>
            <priority>1.0</priority>
        </url>
    {% endfor %}

</urlset>